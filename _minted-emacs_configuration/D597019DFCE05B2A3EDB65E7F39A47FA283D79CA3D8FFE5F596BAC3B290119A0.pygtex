   \PYG{p}{(}\PYG{n+nv}{ipython} \PYG{o}{.} \PYG{n+no}{t}\PYG{p}{)}
   \PYG{p}{(}\PYG{n+nv}{shell} \PYG{o}{.} \PYG{n+no}{t}\PYG{p}{)}
   \PYG{p}{(}\PYG{n+nv}{emacs\PYGZhy{}lisp} \PYG{o}{.} \PYG{n+no}{t}\PYG{p}{)}
   \PYG{p}{))}

\PYG{c+c1}{;; Don\PYGZsq{}t ask before executing code in the chunck.}
