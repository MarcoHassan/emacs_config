\PYG{n+nv}{\PYGZca{}\PYGZca{}I}\PYG{l+s}{\PYGZdq{}xelatex \PYGZhy{}shell\PYGZhy{}escape \PYGZhy{}interaction nonstopmode \PYGZhy{}output\PYGZhy{}directory \PYGZpc{}o \PYGZpc{}f\PYGZdq{}}
\PYG{n+nv}{\PYGZca{}\PYGZca{}I}\PYG{l+s}{\PYGZdq{}xelatex \PYGZhy{}shell\PYGZhy{}escape \PYGZhy{}interaction nonstopmode \PYGZhy{}output\PYGZhy{}directory \PYGZpc{}o \PYGZpc{}f\PYGZdq{}}\PYG{p}{))}

\PYG{p}{(}\PYG{n+nv}{add\PYGZhy{}to\PYGZhy{}list} \PYG{l+s+ss}{\PYGZsq{}org\PYGZhy{}latex\PYGZhy{}packages\PYGZhy{}alist} \PYG{o}{\PYGZsq{}}\PYG{p}{(}\PYG{l+s}{\PYGZdq{}\PYGZdq{}} \PYG{l+s}{\PYGZdq{}minted\PYGZdq{}}\PYG{p}{))}
\PYG{p}{(}\PYG{k}{setq} \PYG{n+nv}{org\PYGZhy{}latex\PYGZhy{}listings} \PYG{l+s+ss}{\PYGZsq{}minted}\PYG{p}{)}

\PYG{c+c1}{;; Allow code evaluation in the org mode source code chunck}

\PYG{p}{(}\PYG{n+nb}{use\PYGZhy{}package} \PYG{n+nv}{gnuplot}\PYG{p}{)} \PYG{c+c1}{;; recall to download it trough MELPA at first}

\PYG{p}{(}\PYG{n+nv}{org\PYGZhy{}babel\PYGZhy{}do\PYGZhy{}load\PYGZhy{}languages}
 \PYG{l+s+ss}{\PYGZsq{}org\PYGZhy{}babel\PYGZhy{}load\PYGZhy{}languages}
 \PYG{o}{\PYGZsq{}}\PYG{p}{((}\PYG{n+nv}{C} \PYG{o}{.} \PYG{n+no}{t}\PYG{p}{)}
   \PYG{p}{(}\PYG{n+nv}{R} \PYG{o}{.} \PYG{n+no}{t}\PYG{p}{)}
